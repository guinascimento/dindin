#content	#profile_container		%table#calendar
			%tr
				%td{:id => current_month?(1) }
					= link_to 'January', '1/invoices/list'
				%td{:id => current_month?(2) }
					= link_to 'February', '2/invoices/list'
				%td{:id => current_month?(3) }
					= link_to 'March', '3/invoices/list'
				%td{:id => current_month?(4) }
					= link_to 'April', '4/invoices/list'
				%td{:id => current_month?(5) }
					= link_to 'May', '5/invoices/list'
				%td{:id => current_month?(6) }
					= link_to 'June', '6/invoices/list'
