				%td{:id => current_month?(7) }
					= link_to 'July', '7/invoices/list'
				%td{:id => current_month?(8) }
					= link_to 'August', '8/invoices/list'
				%td{:id => current_month?(9) }
					= link_to 'October', '9/invoices/list'
				%td{:id => current_month?(10) }
